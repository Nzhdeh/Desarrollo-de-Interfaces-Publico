<Page
    x:Class="_11_EjercicioControlesUWP.Pagina2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:_11_EjercicioControlesUWP"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

 
        
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="Auto"/>
        </Grid.ColumnDefinitions>
            
        <StackPanel Grid.Row="0" Grid.Column="0">
            <AppBarButton x:Name="atras" Icon="Back" Click="Back_Click"  Margin="15,15,15,15"></AppBarButton>
        </StackPanel>

        <StackPanel Grid.Row="0" Grid.Column="1">
            <PersonPicture
            DisplayName="Betsy Sherman"
            ProfilePicture="Assets\imagen.jpg"
            Initials="BS" />
        </StackPanel>
    </Grid>
</Page>
